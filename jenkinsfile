pipeline {
  agent any
  
  stages {
    stage('git') {
      steps {
        git 'https://github.com/praveenkumar57/game-of-life.git'
        }
      }
    stage('build') {
      steps {
        sh 'mvn clean package'
        }
      }
    stage('trigger') {
      steps {
        build job: 'Spring'
      }
    }
  }
}
