pipeline {
  agent any
  parameters {
        string defaultValue: 'master', name: 'mybranch'
      }
  stages {
    stage('git') {
      steps {
        git branch: "${params.mybranch}", url:'https://github.com/praveenkumar57/game-of-life.git'
        }
      }
    stage('build') {
      steps {
        sh 'mvn clean package'
        }
      }
    stage('trigger') {
      steps {
        build job: 'Spring', parameters: [string(name: "${params.mybranch}")]
      }
    }
  }
}
